class B is
    var str: String
    this() is end
    void b() is
        StdIO().PrintLn("this is from B")
        return void
    end
end

class D is
    var int: Integer
    this() is end
    void d() is
        StdIO().PrintLn("this is from D")
        return void
    end
end

class C extends B, D is
    this() is end
    void c() is
        StdIO().PrintLn("this is from C")
        return void
    end
end

class A extends C is
    this() is end
    void b() is
        StdIO().PrintLn("this was from B but now it's from A!")
        return void
    end
end

class Program is
    this() is
        var a: A := A()
        a.str := "str"
        a.int := 1
        a.c()
        a.d()
        a.b()
        StdIO().PrintLn(a.str)
        StdIO().PrintLn(a.int)
    end
end
