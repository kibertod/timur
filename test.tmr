class A[T] is
    this() is end

    // create a B[T] from T
    B[T] MakeB(T: v) is
        return B[T](v)
    end

    // ask a B[T] to produce a T
    T Ask(B[T]: b) is
        return b.Get()
    end
end

class B[T] is
    var value: T

    this(T: v) is
        this.value := v
    end

    // produce the stored value
    T Get() is
        return this.value
    end

    // use A[T] to produce a B[T] from T
    B[T] MakeFromA(A[T]: a, T: v) is
        return a.MakeB(v)
    end
end

class Program is
    this() is
        var a: A[Integer] := A[Integer]()
        var b: B[Integer] := a.MakeB(5)
        StdIO().PrintLn(b.Get())         // prints "5"
        var x: Integer := a.Ask(b)
        StdIO().PrintLn(x)              // prints "5"
    end
end

