class A[T] is
    this() is end

    B[T] MakeB(T: v) is
        return B[T](v)
    end

    T Ask(B[T]: b) is
        return b.Get()
    end
end

class B[T] is
    var value: T

    this(T: v) is
        this.value := v
    end

    T Get() is
        return this.value
    end

    B[T] MakeFromA(A[T]: a, T: v) is
        return a.MakeB(v)
    end

    void Lal() is
        return void
    end
end

class Program is
    this() is
        var a: A[Integer] := A[Integer]()
        var b: B[Integer] := a.MakeB(5)
        b.Lal()
        StdIO().PrintLn(b.Get())
        var x: Integer := a.Ask(b)
        StdIO().PrintLn(x)
    end
end

