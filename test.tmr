class ReversableList[T] extends List is
    this() is super() end
    Void Reverse() is
        var i: Integer := 0
        while (i.LessEq(this.Size().Divide(2))) loop
            this.Swap(i, this.Size().Minus(i.Plus(1)))
        end
    end
end

class Program is
    this() is
        var list: ReversableList[Integer] := ReversableList[Integer]()
        list.Add(1)
        list.Add(2)
        list.Reverse()
    end
end

