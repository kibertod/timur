class Util is
    this() is end
    void Mutate(Integer*: int) is
        *int := int.Plus(10)
        return void
    end
end

class SelfMutable is
    var val: Integer
    this(Integer: val) is
        this.val := val
    end
    void Increment() is
        this.val := this.val.Plus(1)
        return void
    end
end

class Program is
    this() is
        var ptr: SelfMutable* := SelfMutable(1)
        StdIO().PrintLn(ptr.val)
        ptr.Increment()
        StdIO().PrintLn(ptr.val)

        var int: Integer* := 1
        Util().Mutate(int)
        StdIO().PrintLn(*int)
    end
end
