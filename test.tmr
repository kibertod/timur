class Storage is
    var str: String
    var int: Integer

    this(String: string, Integer: int) is
        this.str := string
        this.int := int
    end

    this(Integer: int) is
        this.str := "undefined"
        this.int := int
    end

    Void Print() is
        StdIO().Print(this.str.Concat(": "))
        StdIO().PrintLn(this.int)
    end

    Void Print(String: sep) is
        StdIO().Print(this.str.Concat(sep))
        StdIO().PrintLn(this.int)
    end

    String Return() is
        return this.str.Concat(": ")
    end
end

class Program is
    this() is
        var stdio: StdIO := StdIO()

        if true.Xor(true) then
            stdio.PrintLn("false blin")
        elif false.Or(true) then
            stdio.PrintLn("Unexpected")
        elif true then
            stdio.PrintLn("I know that")
        else
            stdio.PrintLn("Unexpected")
        end

        var i: Integer := 0
        while i.Great(10).Not().And(true) loop
            stdio.Print(i)
            if i.Eq(10).Not() then stdio.Print(", ") end
            i := i.Plus(1)
        end
        stdio.PrintLn("")

        var storage: Storage := Storage("number", 14)
        storage.Print()
        storage.Print(" + ")
        Storage(89725).Print()
        stdio.PrintLn(storage.str)

        storage.int := 0
        storage.Print()
        while storage.int.Great(1000).Not() loop
            storage.int := storage.int.Plus(1)
            storage.Print()
        end
    end
end
